function showTagRecommendLayer() {
	$Element($("tag_recom_layer")).show();
	$Element($("tag_recom_dim")).show();
	var os = $Agent().os();
	var version = parseFloat(os.version || 0);
	var bAndroid40 = os.android && version <= 4.1 && version > 3.0;	
	if ($Agent().navigator().mobile && !bAndroid40) {
		var welBody = $Element(document.body);
		welBody.css("position", "fixed");
		welBody.css("width", "100%");
	}
	
}

function hideTagRecommendLayer() {
	$Element($("tag_recom_layer")).hide();
	$Element($("tag_recom_dim")).hide();
	var os = $Agent().os();
	var version = parseFloat(os.version || 0);
	var bAndroid40 = os.android && version <= 4.1 && version > 3.0;
	if ($Agent().navigator().mobile && !bAndroid40) {
		var welBody = $Element(document.body);
		welBody.css("position", "");
		welBody.css("width", "");
	}
	
	if (os.version == "4.0.4") {
		setTimeout(function() {
			//scrollTo(0,0);
			$Element(document.body).fireEvent("click");
			var nOldScrollTop = $Document().scrollPosition().top;
			scrollTo(0,1);
			scrollTo(0,2);
			scrollTo(0, nOldScrollTop);
		}, 300);
	}
}

function limitLongPostAuthorComment() {
	var elAuthorCommentButton = $$.getSingle(".writer_say .__author_post_comment");
	if (!!elAuthorCommentButton) {
		var elAuthorComment = $$.getSingle("p", elAuthorCommentButton);
		var welAuthorComment = $Element(elAuthorComment);
		
		var isAuthorCommentLong = !!welAuthorComment && welAuthorComment.height() > 38;
		if (isAuthorCommentLong) {
			$Element(elAuthorCommentButton).removeClass("op");
			welAuthorComment.addClass("ell");
			welAuthorComment.addClass("long");
			
			$Fn(function() {
				$Element(elAuthorCommentButton).toggleClass("op");
				welAuthorComment.toggleClass("ell");
				welAuthorComment.toggleClass("long");
			}).attach(elAuthorCommentButton, "click");
		} else {
			$Element(elAuthorCommentButton).addClass("op");
			welAuthorComment.removeClass("ell");
			welAuthorComment.removeClass("long");
		}
	}
}

function attachEventHandlerOfAdjustColumnsByDocumentWidth() {
	var welSect = $Element("__postColumn1");
	var welSectUl = $Element("__postColumn1Ul");
	var aWelHideOnMobile = $ElementList(".__hide_on_mobile");

	function adjustColumnsByDocumentWidth() {
		var w1width = !!welSect ? welSect.width() : 0;

		if (w1width >= 540) {
			aWelHideOnMobile.css("display", "");
		} else {
			aWelHideOnMobile.css("display", "none");
		}
	}
	adjustColumnsByDocumentWidth();
		
	var __delayMs = 200;
	var __fnQueued = null;
	var lastRunTimestamp = null;

	var __runRateLimited = function(evt) {
		Date.now = Date.now || function() { return +new Date; }; 
		var now = Date.now();
		lastRunTimestamp = lastRunTimestamp || now;
		var timeElapsed = now - lastRunTimestamp;

		var runNow = !__fnQueued || (timeElapsed > __delayMs);
		clearTimeout(__fnQueued);

		if (runNow) {
			lastRunTimestamp = now;
			__fnQueued = setTimeout(adjustColumnsByDocumentWidth, 0);
			// console.log("run now...")
		} else {
			lastRunTimestamp = now;
			var wait = __delayMs - timeElapsed;
			__fnQueued = setTimeout(adjustColumnsByDocumentWidth, wait);
			// console.log("wait " + wait + " ms to run")
		}
	}
	
	$Element(window).attach("resize", __runRateLimited);
	$Element(window).attach("orientationchange", __runRateLimited);
}

function attachEventApplyButtonHandler() {
	var elCancel = $$.getSingle(".sect_follow_event button.btn_follow_event.__cancel");
	var welCancel = $Element(elCancel);
	var elApplyCount = $$.getSingle("span i", elCancel);
	var welApplyCount = $Element(elApplyCount);
	
	var elApply = $$.getSingle(".sect_follow_event button.btn_follow_event.__apply");
	var welApply = $Element(elApply);
	!!welApply && welApply.attach("click", function(ev) {
		if (!mug.$.User.isLoggedIn) {
			mug.common.checkLogin();
			return;
		}
		if (welApply.hasClass("__requesting")) {
			alert("응모 요청중입니다.");
			return;
		} else {
			welApply.addClass("__requesting");
		}
		new $Ajax("/viewer/applyUserEvent.nhn", {
			"method": "post",
			"onload": function(resp) {
				var json = resp.json();
				
				if (json.success) {
					if (json.eventPreAction == 'FOLLOW') {
						mug.contents.util.followHide($$(".__followAnchor"));
						mug.contents.util.followShow($$(".__unfollowAnchor"));
					}
					welApply.hide();
					welApply.removeClass("__requesting");
					
					var newApplyCount = Number(welApplyCount.html()) + 1;
					welApplyCount.html(newApplyCount);
					welCancel.css("display", "");
				}
				alert(json.message);
				welApply.removeClass("__requesting");
			},
			"onerror": function() {
				alert("요청 중에 오류가 발생했습니다.");
				welApply.removeClass("__requesting");
			},
			"timeout": 10,
			"ontimeout": function() {
				alert("요청 시간이 초과되었습니다.");
				welApply.removeClass("__requesting");
			}
		}).request({
			"eventNo": welApply.attr("data-eventNo"),
			"authorNo": welApply.attr("data-authorNo")
		});
	});
	
	!!welCancel && welCancel.attach("click", function(ev) {
		if (!mug.$.User.isLoggedIn) {
			mug.common.checkLogin();
			return;
		}
		if (welCancel.hasClass("__requesting")) {
			alert("취소 요청중입니다.");
			return;
		} else {
			welCancel.addClass("__requesting");
		}
		new $Ajax("/viewer/cancelApplyUserEvent.nhn", {
			"method": "post",
			"onload": function(resp) {
				var json = resp.json();
				
				if (json.success) {
					if (json.eventPreAction == 'FOLLOW') {
						mug.contents.util.followHide($$(".__unfollowAnchor"));
						mug.contents.util.followShow($$(".__followAnchor"));
					}
					welCancel.hide();
					welCancel.removeClass("__requesting");
					
					var newApplyCount = Number(welApplyCount.html()) - 1;
					welApplyCount.html(newApplyCount);
					welApply.css("display", "");
				} else if (json.notLogin) {
					alert(location.href);
				} else {
					alert(json.message);
				}
				welCancel.removeClass("__requesting");
			},
			"onerror": function() {
				alert("요청 중에 오류가 발생했습니다.");
				welCancel.removeClass("__requesting");
			},
			"timeout": 10,
			"ontimeout": function() {
				alert("요청 시간이 초과되었습니다.");
				welCancel.removeClass("__requesting");
			}
		}).request({
			"eventNo": welCancel.attr("data-eventNo"),
			"authorNo": welCancel.attr("data-authorNo")
		});
	});
}

