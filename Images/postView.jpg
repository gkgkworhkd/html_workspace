var ViewerFont = {
		/**
		 * 전체선택자 css사용하도록.  
		 * @NOTE
		 * 폰트 사이즈가 1,2,3단계 이상으로 많아질 경우 단계별로 css파일 분리하여야 함.
		 */
		sizeUpFont : function() {
			if (this.fontSize === "1") {
				return;
			}
			
			this._setFontSize("1");
		},
		
		sizeDownFont: function() {
			if (this.fontSize === "0") {
				return;
			}
			
			this._setFontSize("0");
		},
		
		_setFontSize: function(sFontSize) {
			this.fontSize = sFontSize;
			window.se.module.changeDocFontSize(sFontSize);
			var localCache = mug.common.util.getLocalCache();
			localCache.set("viewer_fontSizeType", sFontSize);
			this._setEnabledFontBt();
		},
		
		_configureFontEnv: function() {
			var localCacheFontSizeType = mug.common.util.getLocalCache().get("viewer_fontSizeType");
			var defaultFontSizeType = 0;
			var fontSizeType = localCacheFontSizeType == null ? defaultFontSizeType : localCacheFontSizeType;
			
			this._setEnabledFontBt(); 
		},
		
		_setEnabledFontBt: function() {
			if (mug.$.UserAgent.isInapp) return;
			/**
			 * 폰트사이즈 설정 여부에 따라 disabled / hide 처리
			 */
			var cachedType = mug.common.util.getLocalCache().get("viewer_fontSizeType") || "0";
			
			var welSizeUp = $Element($$.getSingle("a.__viewer_size_up_font"));
			var welSizeDown = $Element($$.getSingle("a.__viewer_size_down_font"));
			
			if (cachedType === "0") {
				this.fontSize = "0";
				!!welSizeUp && welSizeUp.addClass("on");
				!!welSizeDown && welSizeDown.removeClass("on");
			} else {
				this.fontSize = "1";
				!!welSizeUp && welSizeUp.removeClass("on");
				!!welSizeDown && welSizeDown.addClass("on");
			}
		}
};

var __viewer = ViewerFont;
</script>
